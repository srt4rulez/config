# .gitconfig

[user]
    name = Jake Dennison
    email = 

[color]
    ui = auto

    [color "branch"]
        current = yellow reverse
        local = yellow
        remote = green
    [color "status"]
        added = yellow
        changed = green
        untracked = cyan

[core]
    autocrlf = input
    filemode = false

[push]
    default = simple

[pull]
    rebase = true
